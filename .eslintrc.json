{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/first": "error",
    "import/no-duplicates": "error",
    "import/newline-after-import": "error"
  },
  "settings": {
    "import/resolver": {
      "jsconfig": {
        "config": "jsconfig.json"
      }
    }
  }
}
